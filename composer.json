    "description": "Zend Framework 2 ZfcUser LDAP Authentication. this is unofficial version. Please use Nitecon/zfcuser-ldap",
    "type": "library",
    "keywords": ["zf2","zfcuser","ldap"],
    "homepage": "https://github.com/marks12/zfcuser-ldap.git",
    "license": "BSD-4-Clause",
    "authors": [
        {
            "name": "Will Hattingh",
            "email": "w.hattingh@nitecon.com",
            "homepage": "http://www.nitecon.com/",
            "role": "Master"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.*",
        "zf-commons/zfc-user": "1.2.*",
        "bjyoungblood/bjy-authorize": "1.*",
        "doctrine/doctrine-orm-module": "0.*"
    },
    "require-dev": {
	"squizlabs/php_codesniffer":          "1.4.*",
        "satooshi/php-coveralls":             "~0.6"
    },
    "autoload": {
        "psr-0": {
